programa {
  funcao inicio() {
    escreva ("Por favor, digite seu nome: ")
    cadeia nome, idade, altura
    leia (nome)
    escreva ("Por favor, digite sua idade: ")
    leia (idade)
     escreva ("Por favor, digite sua altura: ")
     leia (altura)
     escreva ("Seu nome é: ", nome, "\n", "Sua idade é: ", idade, "\n", "Sua altura é: ", altura)
  }
}
